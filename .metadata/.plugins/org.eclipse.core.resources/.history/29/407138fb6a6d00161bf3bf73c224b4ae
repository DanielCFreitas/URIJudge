package urijudge.iniciante;

import java.util.Scanner;

public class SomaSimples {

	public static void main(String[] args) {
		int A, B, SOMA;
		Scanner read = new Scanner(System.in);
		
		A = read.nextInt();
		B = read.nextInt();
		
		SOMA = A + B;
		
		System.out.println("SOMA = "+SOMA);
		read.close();
	}

}
